{
    "name": "twilight",
    "version": "0.0.0",
    "author": "badkaikai@gmail.com",
    "description": "Visual Metrics GUI",
    "license": null,
    "main": "./dist/main.js",
    "scripts": {
        "build:pack": "make -j2 build-pack",
        "build:dist": "make -j2 build-dist",
        "lint": "make lint",
        "dev": "make dev"
    },
    "build": {
        "productName": "twilight",
        "appId": "twilight",
        "directories": {
            "output": "app"
        },
        "files": [
            "dist/**/*"
        ],
        "dmg": {
            "contents": [
                {
                    "x": 410,
                    "y": 150,
                    "type": "link",
                    "path": "/Applications"
                },
                {
                    "x": 130,
                    "y": 150,
                    "type": "file"
                }
            ]
        },
        "mac": {
            "identity": null
        }
    },
    "dependencies": {
        "chart.js": "2.6.0",
        "concat-stream": "1.6.0",
        "electron-log": "2.2.7",
        "element-ui": "1.3.7",
        "lodash": "4.17.4",
        "vue": "2.4.1",
        "vue-electron": "1.0.6",
        "vue-router": "2.7.0"
    },
    "devDependencies": {
        "babel-core": "6.25.0",
        "babel-eslint": "7.2.3",
        "babel-loader": "7.1.1",
        "babel-plugin-transform-runtime": "6.23.0",
        "babel-preset-env": "1.6.0",
        "babel-preset-stage-0": "6.24.1",
        "babili-webpack-plugin": "0.1.2",
        "copy-webpack-plugin": "4.0.1",
        "css-loader": "0.28.4",
        "devtron": "1.4.0",
        "electron": "1.7.4",
        "electron-builder": "19.16.0",
        "electron-debug": "1.2.0",
        "electron-devtools-installer": "2.2.0",
        "eslint": "4.2.0",
        "eslint-friendly-formatter": "3.0.0",
        "eslint-loader": "1.9.0",
        "eslint-plugin-html": "3.1.1",
        "file-loader": "0.11.2",
        "html-webpack-plugin": "2.29.0",
        "node-sass": "4.5.3",
        "sass-loader": "6.0.6",
        "spectron": "3.7.2",
        "style-loader": "0.18.2",
        "vue-html-loader": "1.2.4",
        "vue-loader": "12.2.2",
        "vue-style-loader": "3.0.1",
        "vue-template-compiler": "2.4.1",
        "webpack": "3.3.0",
        "webpack-dev-server": "2.5.1",
        "webpack-merge": "4.1.0"
    }
}
